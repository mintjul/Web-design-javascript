//tehtävä 2
function repeatingText() {
  for (let i = 1; i <= 50; i++) {
    console.log("If I had nine hours to chop down a tree, I'd spend the first six sharpening my ax. -- Abraham Lincoln" + i);
  }
}

repeatingText();

